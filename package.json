{
  "name": "dieta-positiva",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered wellness coaching platform",
  "author": "Virgilio",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "NODE_OPTIONS='--dns-result-order=ipv4first' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "test:ui": "playwright test",
    "test:ui:headed": "playwright test --headed",
    "test:agents": "vitest src/__tests__/agents",
    "test:contracts": "vitest src/__tests__/contracts",
    "test:simulation": "vitest src/__tests__/simulation",
    "test:drift": "vitest src/__tests__/drift",
    "test:all": "npm run test && npm run test:ui",
    "quality:gates": "bash scripts/quality-gates.sh",
    "setup:sentry": "bash scripts/setup-sentry.sh",
    "verify:setup": "bash scripts/verify-setup.sh",
    "docker:dev": "docker-compose up",
    "docker:build": "docker build -f .docker/Dockerfile.prod -t dp-mvp .",
    "validate": "npm run type-check && npm run lint",
    "build:vercel": "vercel build",
    "deploy": "vercel build && vercel deploy --prebuilt",
    "claude:verify": "node scripts/verify-claude-setup.js",
    "run:agent": "npx tsx src/agents/run_agent.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-dropdown-menu": "^2.0.0",
    "@radix-ui/react-radio-group": "^1.1.0",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tooltip": "^1.0.0",
    "@sentry/nextjs": "^10.32.1",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.17.0",
    "@vercel/analytics": "^1.6.1",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.0",
    "framer-motion": "^11.0.0",
    "groq-sdk": "^0.37.0",
    "lucide-react": "^0.300.0",
    "next": "^14.2.0",
    "openai": "^6.15.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "recharts": "^2.10.0",
    "rimraf": "^6.1.2",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/react": "^16.3.1",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "jsdom": "^27.4.0",
    "playwright": "^1.57.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0",
    "vercel": "^25.2.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
