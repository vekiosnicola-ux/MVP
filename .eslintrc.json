{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "no-eval": "error",
    "no-implied-eval": "error",
    "no-new-func": "error",
    "no-script-url": "error",
    "no-console": "off",
    "prefer-const": "warn",
    "no-var": "error",
    "object-shorthand": "off",
    "prefer-template": "off",
    "react/self-closing-comp": "off",
    "react/jsx-boolean-value": "off",
    "react/jsx-curly-brace-presence": "off",
    "react/no-unescaped-entities": "off",
    "import/order": "off"
  }
}
